<zimlet name="com_zimbra_birthdayreminder" version="0.3" description="Birthday Reminder">
  <include>birthdayreminder.js</include>
  <includeCSS>birthdayreminder.css</includeCSS>
  <handlerObject>com_zimbra_birthdayreminder</handlerObject>

  <zimletPanelItem label="Birthday Reminder" icon="birthdayreminder-panelIcon">
    <toolTipText>Scans Address Book for Birthdays and creates multiple Reminders</toolTipText>
  </zimletPanelItem>

	<userProperties>
		<property type="string" name="bReminder_hasFolder" value="false"/>
	</userProperties>
</zimlet>
