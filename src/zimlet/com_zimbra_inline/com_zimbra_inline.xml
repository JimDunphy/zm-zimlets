<zimlet name="com_zimbra_inline" version="1.0" description="Inline URL">
	<include>inline.js</include>
	<handlerObject>Com_Zimbra_Inline</handlerObject>

    <portlet>
        <canvas title="${msg.title}" width="100%" height="100%" />
        <portletProperties>
          <property type="string" name="url" label="${msg.url}" value="about:blank" />
          <property type="number" name="refresh" label="${msg.refresh}" value="3600000" />
        </portletProperties>
    </portlet>
</zimlet>
